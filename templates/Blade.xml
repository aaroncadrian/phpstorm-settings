<templateSet group="Blade">
  <template name="@y" value="@yield('$YieldName$')" description="Blade Yield" toReformat="false" toShortenFQNames="true">
    <variable name="YieldName" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="@sec" value="@section('$Name$')&#10;    $END$&#10;@endsection" description="Blade Section" toReformat="false" toShortenFQNames="true">
    <variable name="Name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="@inc" value="@include('$END$')" description="Blade Include" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>
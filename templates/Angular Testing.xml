<templateSet group="Angular Testing">
  <template name="tb" value="TestBed.configureTestingModule({&#10;  $END$&#10;});" description="Set up Test Bed" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="pro" value="providers: [&#10;  $END$&#10;]," description="Add providers array" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="proval" value="{&#10;  provide: $TOKEN$,&#10;  useValue: $VALUE$,&#10;},$END$" description="Add provider with value" toReformat="false" toShortenFQNames="true">
    <variable name="TOKEN" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="cspo" value="createSpyObj&lt;$TYPE$&gt;(['$END$'])" description="Create spy object" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="tbg" value="TestBed.get($TOKEN$);$END$" description="TestBed.get(...)" toReformat="false" toShortenFQNames="true">
    <variable name="TOKEN" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>